include ../src/make.sys

.SUFFIXES :
.SUFFIXES : .o .f90
.SUFFIXES : .o .c

all:fourier corplot

fourier:fourier.o key2lower.o
	$(F90) $^ $(LIBS) -o $@

corplot:corplot.o
	$(F90) $^ $(LIBS) -o $@

.f90.o:
	$(F90) -c $< $(FFLAGS)

.c.o:
	$(CC) $(CFLAGS) -c $<

clean:
	rm -f *.o *.mod fourier corplot
